  "scripts": {
    "dev": "nest start --watch",
    "dev:boomemory": "nest start boomemory --watch",
    "dev:boomart": "nest start boomart --watch",
    "dev:boomoney": "nest start boomoney --watch",
    "dev:bot": "nest start bot --watch",
    "build": "nest build",
    "build:boomemory": "nest build boomemory",
    "build:boomart": "nest build boomart",
    "build:boomoney": "nest build boomoney",
    "build:bot": "nest build bot",
    "start": "node dist/apps/gateway/main",
    "start:boomemory": "node dist/apps/boomemory/main",
    "start:boomart": "node dist/apps/boomart/main",
    "start:boomoney": "node dist/apps/boomoney/main",
